<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dl.dao.CityDAO">
    <select id="selectCity" parameterType="City" resultType="City">
            SELECT id,code,name,provinceid FROM city where provinceid = #{provinceid}
    </select>

</mapper>