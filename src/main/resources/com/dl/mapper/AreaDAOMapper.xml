<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dl.day1.AreaDAO">
    <select id="selectArea" parameterType="Area" resultType="Area">
        SELECT id,code,name,cityid FROM area where cityid = #{cityid}
    </select>

</mapper>